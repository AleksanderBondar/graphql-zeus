{
  "name": "graphql-zeus",
  "version": "5.2.0",
  "scripts": {
    "generate-typescript-node": "node ./lib/index.js https://faker.graphqleditor.com/a-team/olympus/graphql examples/typescript-node/src --n -g examples/typescript-node/zeus.graphql --apollo --reactQuery --stuccoSubscriptions --td",
    "start": "ttsc --build tsconfig.build.json --watch",
    "cli": "node ./lib/CLI/index.js",
    "build": "ttsc --build tsconfig.build.json"
  },
  "author": "",
  "license": "MIT",
  "description": "Generate Client Library for GraphQL Schema",
  "homepage": "https://graphqleditor.com",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "zeus": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-editor/graphql-zeus.git"
  },
  "bugs": {
    "url": "https://github.com/graphql-editor/graphql-zeus.git"
  },
  "private": "false",
  "dependencies": {
    "yargs": "^16.1.1",
    "cross-fetch": "^3.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
